<mujoco>
  <compiler meshdir="phantoms/phantom-elastrat"></compiler>
  <visual>
    <global elevation="-30" linewidth="0.001" />
    <scale forcewidth="0.005" contactwidth="0.005" connect="0.001" com="0.01" selectpoint="0.001"
      jointlength="0.01" jointwidth="0.005" framelength="0.01" framewidth="0.005" />
  </visual>
  <default>
    <site group="0" size="0.005" type="sphere" />
  </default>
  <compiler angle="degree"></compiler>
  <option cone="elliptic" density="1055" integrator="implicitfast" jacobian="sparse"
    o_margin="0.004" o_solimp="0.9 0.95 0.001 0.5 6" o_solref="0.01 0"
    solver="PGS" timestep="0.002" viscosity="0.004">
    <flag frictionloss="disable"></flag>
  </option>
  <include file="guidewires/guidewire_angled.xml"></include>
  <include file="phantoms/phantom-elastrat/phantom-elastrat.xml"></include>
  <statistic meansize=".05"></statistic>
  <visual>
    <rgba haze="0.15 0.25 0.35 1"></rgba>
    <quality shadowsize="4096"></quality>
    <map fogend="15" fogstart="1" haze="1" shadowscale="0.5" stiffness="700" zfar="40"></map>
  </visual>
  <asset>
    <!-- <texture builtin="gradient" height="512" rgb1="0.3 0.5 0.7" rgb2="0 0 0" type="skybox" -->
    <!--   width="512"></texture> -->
    <texture builtin="checker" height="512" mark="cross" markrgb=".8 .8 .8" name="texplane"
      rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" type="2d" width="512"></texture>
    <material name="matplane" reflectance="0.3" texrepeat="10 10" texture="texplane"
      texuniform="true"></material>
    <texture type="skybox" builtin="gradient" rgb1="1 1 1" rgb2="1 1 1" width="256"
      height="256"></texture>
  </asset>
  <worldbody>
    <camera name="top" pos="-0.027 0.1 0.29" />
    <light castshadow="false" pos="0 0 10" dir="20 20 -20"></light>
  </worldbody>
</mujoco>
